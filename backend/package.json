{
  "name": "backend",
  "version": "1.0.0",
  "description": "local env backend + cloud functions",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "dev": "npm run build && npm run firebase emulators:start",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve:force": "npm run build && npm run firebase emulators:start --only functions",
    "shell": "npm run build && npm run firebase functions:shell",
    "firebase": "firebase",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "author": "",
  "license": "ISC",
 
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "firebase-tools": "^11.22.0",
    "typescript": "^4.5.4"
  },
  "private": true
}
